\begin_layout Plain Layout

colnames(Z) <- rownames(Z) <- c("Apollo","Bridgestone","CEAT","Falken")
\end_layout

\begin_layout Plain Layout

Z
\end_layout

\begin_layout Plain Layout

@
\end_layout

\end_inset


\end_layout

\end_deeper
